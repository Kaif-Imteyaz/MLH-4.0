                //- li: hr.dropdown-divider
                //- li: a.dropdown-item(href="#") Something else here
          ul.navbar-nav.justify-content-end.flex-grow-1.pe-3
            //- Nav-btn
            li.nav-button
              if user 
                a.btn.btn-dark(aria-current="page", href="/me") #{user.name}
              else  
                a.btn.btn-dark(aria-current="page", href="/login") login
